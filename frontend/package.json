{
  "name": "openredact-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.26.0",
    "@blueprintjs/icons": "^3.16.0",
    "axios": "^0.19.2",
    "file-saver": "^2.0.2",
    "node-polyglot": "^2.4.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-hotkeys": "^2.0.0",
    "react-scripts": "3.4.1",
    "react-text-annotate": "^0.3.0",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/react-scripts start",
    "build": "./node_modules/.bin/react-scripts build",
    "test": "./node_modules/.bin/react-scripts test",
    "eject": "./node_modules/.bin/react-scripts eject",
    "lint": "eslint --ext .jsx,.js src/",
    "format": "npx prettier . --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "plugin:react-hooks/recommended",
      "prettier",
      "prettier/react",
      "@blueprintjs/eslint-config"
    ],
    "plugins": [
      "jsx-a11y",
      "react-hooks",
      "@blueprintjs/blueprint"
    ]
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "devDependencies": {
    "@blueprintjs/eslint-config": "^0.3.0",
    "@blueprintjs/eslint-plugin-blueprint": "^0.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "serve": "^11.3.2",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  }
}
